{
  "name": "satelite",
  "description": "Satelite State Management",
  "version": "0.0.0",
  "main": "dist/index.js",
  "esnext": "dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepublish": "yarn run compile",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "tslint src/**/*.ts --fix",
    "compile": "rm -rf dist && tsc --sourceMap --outDir dist --declaration --target es6",
    "compile:watch": "tsc --sourceMap --outDir dist --declaration --watch"
  },
  "license": "Apache-2.0",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "devDependencies": {
    "@types/faker": "^4.1.0",
    "@types/jest": "^19.2.4",
    "@types/uuid": "^3.0.0",
    "faker": "^4.1.0",
    "jest": "^20.0.3",
    "jest-cli": "^20.0.3",
    "ts-node": "^3.1.0",
    "tslint": "^5.3.2",
    "typescript": "^2.4.0",
    "uuid": "^3.1.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.68",
    "@types/react": "^15.0.37",
    "hoist-non-react-statics": "^2.1.0",
    "interstelar": "^0.0.2-0",
    "lodash": "^4.17.4",
    "react": "^15.6.1"
  }
}
