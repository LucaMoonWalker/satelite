{
  "name": "satelite",
  "description": "Satelite State Management",
  "version": "0.0.0",
  "main": "dist/index.js",
  "esnext": "dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepublish": "yarn run compile",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint --project .",
    "lint:fix": "tslint --project . --fix",
    "compile": "rm -rf dist && tsc --sourceMap --outDir dist --declaration",
    "compile:watch": "tsc --sourceMap --outDir dist --declaration --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/faker": "^4.1.2",
    "@types/jest": "^23.0.0",
    "@types/node": "^8.0.25",
    "faker": "^4.1.0",
    "husky": "^1.1.0",
    "jest": "^23.0.0",
    "jest-cli": "^23.0.0",
    "lint-staged": "^7.1.3",
    "normalizr": "^3.2.3",
    "prettier": "^1.13.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.3.2",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.74",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^16.0.5",
    "hoist-non-react-statics": "^3.0.0",
    "interstelar": "^0.0.2-0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^16.0.0"
  }
}
